<?xml version="1.0" encoding="UTF-8"?>

<!ELEMENT shellCommand EMPTY>
<!ATTLIST shellCommand
	name CDATA #REQUIRED
	command CDATA #REQUIRED>
	
<!ELEMENT menu ((menu|shellCommand)+)>
<!ATTLIST menu
	name CDATA #REQUIRED
	foreground (black|blue|cyan|green|magenta|red|white|yellow) #IMPLIED
	background (black|blue|cyan|green|magenta|red|white|yellow) #IMPLIED>
